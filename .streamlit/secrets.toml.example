# Streamlit Secrets Configuration
# Copy this file to secrets.toml and fill in your values
# DO NOT commit secrets.toml to version control!

# Railway PostgreSQL Database URL (PUBLIC URL for external access)
# Format: postgresql://username:password@host:port/database
DATABASE_URL = "postgresql://postgres:YOUR_PASSWORD_HERE@your-host.railway.app:PORT/railway"

# Admin password for the Admin page
ADMIN_PASSWORD = "your-secure-admin-password"
